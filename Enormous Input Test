import java.util.*;
class Divisible
{
    public static void main (String[] args) throws java.lang.Exception
    {
        Scanner s = new Scanner(System.in);
        int b = 0;
        int n = s.nextInt();
        int k = s.nextInt();
        while(n-->0){
            int a = s.nextInt();
            if(a%k==0) {
                b++;
            }
        }
        System.out.println(b);
    }
}
